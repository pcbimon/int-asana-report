# Example environment variables for this Supabase docker-compose setup
# Copy this file to `.env` and fill in real values before starting the stack

# Postgres / DB
POSTGRES_PASSWORD=changeme
POSTGRES_DB=postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
PGPASSWORD=${POSTGRES_PASSWORD}
PGPORT=${POSTGRES_PORT}

# Supabase keys / JWT
JWT_SECRET=supersecretjwtkey
JWT_EXPIRY=3600
ANON_KEY=anon_key_here
SERVICE_ROLE_KEY=service_role_key_here

# Supabase studio / dashboard
STUDIO_DEFAULT_ORGANIZATION=MyOrg
STUDIO_DEFAULT_PROJECT=MyProject
SUPABASE_PUBLIC_URL=http://localhost:3000
API_EXTERNAL_URL=http://localhost:9999

# Auth / SMTP and related
SITE_URL=http://localhost:3000
ADDITIONAL_REDIRECT_URLS=http://localhost:3000
DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_ANONYMOUS_USERS=false
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_PHONE_SIGNUP=false
ENABLE_PHONE_AUTOCONFIRM=false
SMTP_ADMIN_EMAIL=admin@example.com
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=user@example.com
SMTP_PASS=password
SMTP_SENDER_NAME="Supabase"
MAILER_URLPATHS_INVITE=/auth/v1/verify
MAILER_URLPATHS_CONFIRMATION=/auth/v1/confirm
MAILER_URLPATHS_RECOVERY=/auth/v1/recover
MAILER_URLPATHS_EMAIL_CHANGE=/auth/v1/email-change

# Studio / Dashboard credentials
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=password

# Kong / HTTP ports
KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443

# Analytics / Logflare
LOGFLARE_PRIVATE_ACCESS_TOKEN=
LOGFLARE_PUBLIC_ACCESS_TOKEN=

# Realtime / Supavisor / Pooler
SECRET_KEY_BASE=some_secret_key_base
VAULT_ENC_KEY=some_vault_enc_key
POOLER_TENANT_ID=local
POOLER_DEFAULT_POOL_SIZE=10
POOLER_MAX_CLIENT_CONN=100
POOLER_DB_POOL_SIZE=10
POOLER_POOL_MODE=transaction
POOLER_PROXY_PORT_TRANSACTION=6543

# Storage / imgproxy
IMGPROXY_ENABLE_WEBP_DETECTION=true

# Functions / Edge Runtime
FUNCTIONS_VERIFY_JWT=true

# Misc
DOCKER_SOCKET_LOCATION=/var/run/docker.sock
SERVICE_ROLE_KEY=${SERVICE_ROLE_KEY}
ANON_KEY=${ANON_KEY}

# Optional: GCP for analytics (uncomment if using BigQuery)
#GOOGLE_PROJECT_ID=
#GOOGLE_PROJECT_NUMBER=
